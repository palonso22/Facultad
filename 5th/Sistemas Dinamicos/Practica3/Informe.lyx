\end{cases}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $(1-\frac{1}{2}h\lambda)^{-1}(1+\frac{1}{2}h\lambda)<1\iff1+\frac{1}{2}h\lambda<1-\frac{1}{2}h\lambda\iff\frac{1}{2}h\lambda<-\frac{1}{2}h\lambda\iff h\lambda<0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $h>0$
\end_inset

 implica 
\begin_inset Formula $\lambda<0$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $(1-\frac{1}{2}h\lambda)^{-1}(1+\frac{1}{2}h\lambda)>-1\iff1+\frac{1}{2}h\lambda>\frac{1}{2}h\lambda-1$
\end_inset

 lo cual se cumple siempre.
\end_layout

