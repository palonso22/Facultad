library(purrr)
library(dplyr)
args = commandArgs(trailingOnly=TRUE)
#number of samples
n = as.numeric(args[1])
#dimensionality
d = as.numeric(args[2])
#constant
c = as.double(args[3])
#standar deviation
sd = c*sqrt(d)
generator <- function(n,d,mean,var,class){
points <- 1:n %>% map(function(x) c(rnorm(d,mean,var),class))
matrix(unlist(points),n,byrow=TRUE)
}
class0 <- generator(n,d,-1,sd^2,0)
x <- class0[,1]
y <- class0[,2]
plot(x,y)
plot(x,y)
x <- class0[,1]
class0 <- generator(n,d,-1,sd^2,0)
